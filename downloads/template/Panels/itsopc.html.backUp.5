    <style id="ITSOPCSTYLE">
    #ITSOPC div,
    #ITSOPC section,
    #ITSOPC p,
    #ITSOPC ul {
        color: TEXT_COLOR;
    }
    #ITSOPC a, .itsCoupon span {
        color: LINK_COLOR;
    }
    #ITSOPC a:hover, .itsCoupon span:hover {
        color: LINK_HOVER_COLOR;
    }
    #ITSOPC section.column {
        background-color: COLUMN_BACKGROUND_COLOR;
        border-color: COLUMN_BORDER_COLOR;
    }
    #ITSOPC .itsSubmitOrder {
        background-color: CHECKOUT_BUTTON_BACKGROUND !important;
        color: CHECKOUT_BUTTON_COLOR !important;
    }
    #ITSOPC .itsSubmitOrder:hover {
        color: CHECKOUT_BUTTON_HOVER_COLOR !important;
        background-color: CHECKOUT_BUTTON_HOVER_BACKGROUND !important;
    }
    #ITSOPC h2 {
        border-bottom: 3px solid PRIMARY_COLOR;
    }
    #ITSOPC h3 {
        border-bottom: 3px solid PRIMARY_COLOR;
    }
    #ITSOPC h3 span,
    #ITSOPC h2 span {
        background-color: PRIMARY_COLOR;
    }
</style>

    <div id="ITSOPC" style="display: none">
        <div class="OPCHead"><h1></h1></div>
        <!-- Column 1 -->
        <section class="column" id="StepOne">
            <h2 class="CustomerInfo"><span>1</span> Customer Information</h2>
            <div class="logIn">
                <span class="isYou" style="display:none;">Not <span class="custyName">%%GLOBAL_CurrentCustomerFirstName%%</span>? <a href="#">Sign Out</a></span>
                <span class="caption">If you are a returning customer, please log in.</span>
                <ul>
                    <li class="newCustomer"><span class="checker"><span class="checked"></span></span> I am a new customer</li>
                    <li class="returningCustomer"><span class="checker"><span></span></span> I am a returning customer</li>
                </ul>
            </div>
            <div class="loginForm"></div>
            <div id="opcBilling">
                <h3 class="BillingInfo">Billing & Shipping Information</h3>
                <div class="formcontainer"></div>
            </div>
            <div id="opcShipping">
                <h3 class="ShippingInfo">Shipping Information</h3>
                <div class="formcontainer"></div>
            </div>
        </section>
        <!-- Column 2 -->
        <section class="column" id="StepTwo">
            <div>
                <h2 class="ShippingMethod"><span>2</span> Shipping Method</h2>
                <p class="caption">Please enter your shipping details to see available shipping methods</p>
                <div class="itsShippingMethods">
                </div>
            </div>
            <div class="Payment">
                <h3 class="PaymentMethod">Payment Information</h3>
                <div class="itsCCForm">
                    <div class="itsBillingMethods"></div>
                    <div class="cardHolder">
